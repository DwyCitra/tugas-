      "source": [
        "def mod(number, cellNumber):\n",
        "    return number % cellNumber\n",
        "\n",
        "\n",
        "# print(mod(400, 24))\n",
        "\n",
        "\n",
        "def modASCII(string, cellNumber):\n",
        "    total = 0\n",
        "    for i in string:\n",
        "        total += ord(i)\n",
        "    return total % cellNumber\n"
      ]
    },
