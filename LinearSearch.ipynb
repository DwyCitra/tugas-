      "source": [
        "# Searching algorithms - Linear Search\n",
        "\n",
        "def linearSearch(array, value):\n",
        "    for i in range(len(array)):\n",
        "        if array[i] == value:\n",
        "            return i\n",
        "    return -1"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "print(linearSearch([20,40,30,50,90], 90))"
